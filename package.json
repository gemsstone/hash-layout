{
  "name": "hash-layout",
  "version": "1.1.7",
  "description": "The Hash-Layout library is a powerful tool for React developers, providing a rich set of pre-built components that adhere to Google's Material Design guidelines. These components are highly customizable, allowing developers to easily adapt them to suit their project requirements. ",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && npm run copy-files",
    "copy-files": "copyfiles -u 1 src/**/*.css dist/",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gemsstone/hash-layout"
  },
  "keywords": [
    "React",
    "layout",
    "layout components",
    "layout designs",
    "layout styles"
  ],
  "author": "Md Manowar Hashmi <hmdmanowar@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://gemsstone.github.io/hash-layout-documentation/issues"
  },
  "homepage": "https://gemsstone.github.io/hash-layout-documentation/",
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@types/react": "^18.2.61",
    "copyfiles": "^2.4.1",
    "react": "^18.2.0",
    "rimraf": "^5.0.5"
  },
  "files": [
    "dist/**/*"
  ]
}
